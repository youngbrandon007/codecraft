

def fizzbuzz(i):
    modthree = ((i % 3) == 0)
    modfive = ((i % 5) == 0)

    if modfive && modthree:
        print("fizzbuzz")
    elif modfive:
        print("buzz")
    elif modthree:
        print("fizz")
    else:
        print(i)



def test():
    for k in range(1, 16):
        fizzbuzz(k)